<UserControl x:Class="LimpStats.Client.CustomControls.ForStudents.StudentSettingsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LimpStats.Client.CustomControls"
             mc:Ignorable="d" 
             d:DesignHeight="40" d:DesignWidth="180">
    <StackPanel Orientation="Horizontal" >
        <Button x:Name="Title"
                Height="40"
                Width="140"
                HorizontalAlignment="Left"
                Click="Update_User"
                Foreground="#FFE8F9F2"
                Background="#333333"
                FontSize="18"/>
        <Button x:Name="Del"
                Content="✘" 
                Height="40"
                Width="40"
                Style="{DynamicResource Close}" 
                Canvas.Left="140"
                Click="DelUser"
                />
    </StackPanel>
</UserControl>
